<%- headers = ['Campaign Name', 'Post Likes', 'Reach', 'Comments', 'Shares', 'Campaign Starts', 'Start time', 'Campaign Ends', 'End time'] -%>
<%= CSV.generate_line headers %>
<% @campaigns.each do |campaign| %>
    <%= CSV.generate_line([campaign.name, campaign.number_of_likes, campaign.number_of_post_reach_of_post, campaign.number_of_comments, campaign.number_of_shares, campaign.get_start_date, campaign.get_start_time, campaign.get_end_date, campaign.get_end_time]).html_safe.strip %>
<% end %>